---
export interface Props {
	title: string;
}

const { title } = Astro.props;

import '@styles/main.scss';
import { sitePath } from '@scripts/utils';
---

<!DOCTYPE html>
<html lang='en'>
	<head>
		<meta charset='UTF-8' />
		<meta name='viewport' content='width=device-width' />
		<link rel='icon' href={sitePath('/favicon.ico')} />
		<meta name='generator' content={Astro.generator} />
		<title>{title}</title>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css'
			media='all'
			async='async'
		/>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://dev-zoetispets.cphostaccess.com/assets/css/style.css?v=638004728149230000'
			media='all'
			async='async'
		/>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://dev-zoetispets.cphostaccess.com/assets/css/customstylenewfooter.css?v=638004726671030000'
			media='all'
			async='async'
		/>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://dev-zoetispets.cphostaccess.com/assets/css/images.css?v=637557994652770000'
			media='all'
			async='async'
		/>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://dev-zoetispets.cphostaccess.com/assets/css/videobanner.css?v=638004728249600000'
			media='all'
			async='async'
		/>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://use.typekit.net/nfu5tzo.css'
			media='all'
			async='async'
		/>
		<link
			rel='stylesheet'
			type='text/css'
			href='https://pro.fontawesome.com/releases/v5.10.0/css/all.css'
			media='all'
		/>
		<script
			type='text/javascript'
			src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js'
			defer='defer'
			async='async'
		></script>
		<script
			type='text/javascript'
			src='https://dev-zoetispets.cphostaccess.com/assets/js/jquery.min.js?v=637771420535100000'
		></script>
		<link rel='preconnect' href='https://fonts.googleapis.com' />
		<link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />
		<link
			href='https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600;700&family=Roboto:wght@700&display=swap'
			rel='stylesheet'
		/>
	</head>
	<body>
		<header>
			<div class='container'>
				<div class='d-lg-flex flex-row row1'>
					<div class='text-left'>
						<logo class='container'>
							<a href='/uk-en/' class='main-logo'>
								<img class='logo' src='https://dev-zoetispets.cphostaccess.com/assets/images/zoetis-logo.svg' alt='' />
							</a>
						</logo>
					</div>
					<div class=''>
						<site-navigation class='container'>
							<nav class='navbar navbar-expand-lg navbar-light'>
								<button
									class='navbar-toggler js-navbar-toggler'
									data-toggle='collapse'
									type='button'
									data-target='#navbar'
									aria-label='menu'
								>
									<span class='navbar-toggler-icon'></span>
									<span class='navbar-toggler-icon'></span>
									<span class='navbar-toggler-icon'></span>
								</button>
								<div class='collapse navbar-collapse' id='navbar'>
									<ul class='navbar-nav'>
										<li class='nav-item' style='display: block;'>
											<a class='nav-link' href='/uk-en/'> HOME</a>
										</li>
										<li class='nav-item submenu' style='display: block;'>
											<a
												class='nav-link submenu-toggle'
												href='#'
												ga-tag='internal'
												>DOG CARE<span class='icon-arrow'></span>
											</a>
											<div class='submenu-wrapper'>
												<a
													class='submenu-link'
													href='/uk-en/dog/skin-conditions/'
												>
													SKIN CONDITIONS</a
												>
												<a class='submenu-link' href='/uk-en/dog/arthritis/'>
													ARTHRITIS</a
												>
											</div>
										</li>
										<li class='nav-item' style='display: block;'>
											<a class='nav-link' href='/uk-en/cat/arthritis/'>
												CAT CARE</a
											>
										</li>
										<li class='nav-item' style='display: block;'>
											<a
												class='nav-link'
												href='https://www2.zoetis.co.uk/about-us/'
												target='_blank'
												rel='noreferrer'
											>
												ABOUT ZOETIS<i class='external-icon'></i>
											</a>
										</li>
										<li class='nav-item' style='display: block;'>
											<a class='nav-link' href='/'>
												United Kingdom<span>English</span>
											</a>
										</li>
									</ul>
								</div>
							</nav>
						</site-navigation>
					</div>
				</div>
			</div>
		</header>
		<slot />
		<footer class='column'>
			<div class='container'>
				<div class='footer-logo-container'>
					<logo>
						<div class='main-logo'>
							<img
								class='logo'
								src='https://dev-zoetispets.cphostaccess.com/assets/images/zoetis-logo.svg'
								alt='Zoetis logo'
							/>
						</div>
					</logo>
				</div>

				<div class='footer-link--list'>
					<a class='footer-link--item' href='/uk-en/'>Home</a>

					<a
						class='footer-link--item'
						href='https://www2.zoetis.co.uk/about-us/'>About</a
					>
				</div>

				<div class='footer-content'>
					<p class='footer-content--item'>
						Zoetis discovers, develops, manufactures and commercializes a
						diverse portfolio of animal health medicines and vaccines designed
						to meet the real-world needs of veterinarians and the livestock
						farmers and pet owners they support.
					</p>
				</div>

				<div class='footer-cards'>
					<div class='footer-cards--item'>
						<img
							class='footer-cards--icon'
							src='https://dev-zoetispets.cphostaccess.com/assets/images/footer-icons/veterinary@2x.svg'
							alt='footer icon'
						/>
						<p class='footer-cards--text'>
							65+ years of experience in animal health
						</p>
					</div>

					<div class='footer-cards--item'>
						<img
							class='footer-cards--icon'
							src='https://dev-zoetispets.cphostaccess.com/assets/images/footer-icons/worldwide@2x.svg'
							alt='footer icon'
						/>
						<p class='footer-cards--text'>
							100+ Countries in which Zoetis products are sold
						</p>
					</div>

					<div class='footer-cards--item'>
						<img
							class='footer-cards--icon'
							src='https://dev-zoetispets.cphostaccess.com/assets/images/footer-icons/veterinary-(1)@2x.svg'
							alt='footer icon'
						/>
						<p class='footer-cards--text'>
							300 approximate number of product lines
						</p>
					</div>
				</div>

				<div class='footer-content footer-content-cta'>
					<p class='footer-content--item'>
						Visit <a href='https://www2.zoetis.co.uk/'>zoetis.co.uk</a> for more
						information about Zoetis
					</p>
				</div>
			</div>
			<div class='footer-copyright'>
				<p class='footer-copyright--item'>
					Â© Copyright 2022. All trademarks are the property of Zoetis Inc., its
					affiliates and/or its licensors. All other trademarks are the property
					of their respective owners.
				</p>
			</div>
			<div class='footer-links'>
				<footer-links
					class='container footer-class white-small-without-vertical-line'
				>
					<ul>
						<li class=''>
							<a href='/uk-en/privacy-policy'>Privacy Policy</a>
						</li><li class=''>
							<a href='/uk-en/terms-of-use'>Terms of use</a>
						</li><li class=''>
							<a href='#' class='ot-sdk-show-settings'>Cookie settings</a>
						</li><span class='mmno'>MM-18032</span>
					</ul>
				</footer-links>
			</div>
		</footer>
		<script src='../scripts/main.js'></script>
	</body>
</html>
